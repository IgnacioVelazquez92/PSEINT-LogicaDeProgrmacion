Proceso Quinto_1b
	Definir  a,b,c,resultado Como Real;
	
	Escribir "Digite el valor de A: ";
	Leer a;
	
	Escribir "Digite el valor de B: ";
	Leer b;
	
	Escribir "Digite el valor de c: ";
	Leer c;
	
	resultado <- (-b + rc(b^2 - 4*a*c));
	
	Escribir "el resultado de la primer raiz es: " , resultado;
	
	resultado <- (-b - rc(b^2 - 4*a*c));
	
	Escribir "el resultado de la segunda raiz es: " , resultado;
	
FinProceso
