Algoritmo clase1_b_2
	Definir num, num2, resultado como Entero;
	Escribir "Digite un número entero: ";
	Leer num;
	
	Escribir "Digite un segundo número entero: ";
	Leer num2;
	
	resultado <- num + num2;
	
	Escribir "La suma de ambos es : ", resultado;
	
	
FinAlgoritmo
